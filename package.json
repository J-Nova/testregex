		"@rollup/plugin-node-resolve": "^13.3.0",
		"@sveltejs/adapter-node": "^2.0.2",
		"@sveltejs/kit": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/cookie": "^0.5.4",
		"@zerodevx/svelte-toast": "^0.9.5",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
